<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>LoginData</servlet-name>
		<servlet-class>com.goach.web.servlet.LoginDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginData</servlet-name>
		<url-pattern>/LoginDataServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>RegisterData</servlet-name>
		<servlet-class>com.goach.web.servlet.RegisterDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterData</servlet-name>
		<url-pattern>/RegisterDataServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>NewsData</servlet-name>
		<servlet-class>com.goach.web.servlet.NewsDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NewsData</servlet-name>
		<url-pattern>/NewsDataServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
